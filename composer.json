{
    "name": "laradic/ci-core",
    "description": "laradic/ci-core is a laravel package",
    "authors": [
        {
            "name": "Robin Radic",
            "email": "robin@radic.nl",
            "homepage": "http://radic.nl"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laradic/support": "1.0.*",
        "block8/phpci": "1.6.*"
    },
    "require-dev": {
        "laradic/testing": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "Laradic\\CI\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laradic\\CI\\Core\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
